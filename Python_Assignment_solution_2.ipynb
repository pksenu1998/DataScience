   "source": [
    "# 1.Create the below pattern using nested for loop in Python. \n",
    "\n",
    "\n",
    "*  \n",
    "* *\n",
    "* * *  \n",
    "* * * *  \n",
    "* * * * *  \n",
    "* * * *  \n",
    "* * *  \n",
    "* *  \n",
    "*\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "\n",
      "\n",
      "* \n",
      "\n",
      "* * \n",
      "\n",
      "* * * \n",
      "\n",
      "* * * * \n",
      "\n",
      "* * * * * \n",
      "\n",
      "* * * * \n",
      "\n",
      "* * * \n",
      "\n",
      "* * \n",
      "\n",
      "* \n",
      "\n"
     ]
    }
   ],
   "source": [
    "for i in range(0,5,1):\n",
    "  for j in range(i): \n",
    "    print('* ',end='')\n",
    "  print('\\n')\n",
    "\n",
    "for i in range(5,0,-1):\n",
    "  for j in range(i): \n",
    "    print('* ',end='')\n",
    "  print('\\n')"
   ]
  },
